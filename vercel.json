{
  "name": "website",
  "scope": "simonalbrecht",
  "alias": [
    "simonalbrecht.ch",
    "simonalbrecht"
  ],
  "routes": [
    {
      "src": "/cv",
      "status": 308,
      "headers": {
        "Location": "https://cdn.simonalbrecht.ch/file/simonalbrecht-public/cv/cv-simon-albrecht.pdf"
      }
    },
    {
      "src": "/view-source",
      "status": 308,
      "headers": {
        "Location": "https://github.com/simonalbrecht/website"
      }
    },
    {
      "src": "/en/",
      "dest": "/en/index.html"
    },
    {
      "src": "/de/",
      "dest": "/de/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404"
    }
  ]
}
