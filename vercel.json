{
    "name": "website",
    "scope": "simonalbrecht",
    "alias": [
        "simonalbrecht.ch",
        "simonalbrecht"
    ],
    "routes": [
        {
            "src": "/cv",
            "dest": "https://cdn.simonalbrecht.ch/file/simonalbrecht-public/cv/cv-simon-albrecht.pdf"
        },
        {
            "src": "/view-source",
            "dest": "https://github.com/simonalbrecht/website"
        },
        {
            "src": "/de/(.*)",
            "continue": true
        },
        {
            "src": "/en/(.*)",
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "/en/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/404/"
        }
    ]
}
